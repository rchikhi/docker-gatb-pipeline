#!/bin/bash
echo
echo "#####################################################################"
echo " Running Medium_Complexity_Test_Dataset task..."
echo "#####################################################################"
echo
echo "pwd=> `pwd`"
SCRATCH=/dckr/mnt/output/scratchdir
mkdir -p $SCRATCH
cd $SCRATCH && rm -rf *
echo "pwd=> `pwd`"
echo
ls -1 /dckr/mnt/input/*reads_* > list_reads
set -xv
/usr/local/bin/gatb -l $SCRATCH/list_reads --multik-meta

